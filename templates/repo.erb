[<%= @repo_name %>]
<% if defined?(@metadata_expire) -%>
metadata_expire = <%= @metadata_expire %>
<% end -%>
<% if defined?(@baseurl) -%>
baseurl = <%= @baseurl %>
<% end -%>
<% if defined?(@mirrorlist) -%>
mirrorlist=<%= @mirrorlist %>
<% end -%>
<% if defined?(@description) -%>
name = <%= @description %>
<% end -%>
enabled = <%= scope.function_bool2number([@enabled]) %>
gpgcheck = <%= scope.function_bool2number([@gpgcheck]) %>
